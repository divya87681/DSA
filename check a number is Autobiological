import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        String s = sc.next();
        if (isAutobiographical(s)) {
            System.out.println(s + " is an autobiographical number.");
        } else {
            System.out.println(s + " is NOT an autobiographical number.");
        }
    }

    public static boolean isAutobiographical(String s) {
        int n = s.length();
        int[] count = new int[n];

        // Count frequency of each digit
        for (int i = 0; i < n; i++) {
            int digit = s.charAt(i) - '0';
            if (digit < n) {
                count[digit]++;
            } else {
                // If a digit is outside the range, it's not autobiographical
                return false;
            }
        }

        // Check if autobiography condition holds
        for (int i = 0; i < n; i++) {
            int expected = s.charAt(i) - '0';
            if (count[i] != expected) {
                return false;
            }
        }
        return true;
    }
}
